{
  "name": "open-public-data",
  "version": "0.2.0",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon --watch src -e ts --exec ts-node src/server.ts",
    "clean": "./node_modules/.bin/prettier --write '{src,__test__}/**/*.ts'",
    "start": "./node_modules/.bin/ts-node src/server.ts",
    "build": "rimraf ./build && tsc -p tsconfig.json",
    "production": "dezenter build/src/server.js",
    "test": "jest",
    "test:all": "jest",
    "test:unit": "jest",
    "test:feature": "jest",
    "coverage": "jest",
    "typeorm": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "cheerio": "1.0.0-rc.3",
    "commander": "4.1.1",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cron": "1.7.2",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "helmet": "3.21.2",
    "influx": "5.5.1",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "pg": "7.17.1",
    "puppeteer": "2.0.0",
    "reflect-metadata": "0.1.10",
    "typeorm": "0.2.22"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.15",
    "@types/compression": "^1.7.0",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.2",
    "@types/express-rate-limit": "^3.3.3",
    "@types/helmet": "^0.0.45",
    "@types/node": "13.1.7",
    "dotenv": "^8.2.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "3.7.5"
  }
}
